a = 10
b = 5
soma = a + b
subtracao = a - b
multiplicacao = a * b
divisao = a / b

a = True
b = False
resultado1 = a and b
resultado2 = a or b
resultado3 = not a 

c = 3
c == 4
c > 2
c ! = 3
d = 4
c is d
c is not d

